{
  "env": {
    "cypress/globals": true
  },
    "globals": {
        "cy": "readonly",
        "it": "readonly",
        "describe": "readonly"
    },
  "extends": [
     "eslint:recommended",
     "plugin:cypress/recommended"
    // "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["cypress"],
  "rules": {
    // "no-unused-vars": "warn",
    // "semi": ["error", "always"],
    // "quotes": ["error", "single"]
  }
}
